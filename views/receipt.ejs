<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
    <script language="javascript" src="./jquery-3.2.0.min.js" type="text/javascript"></script>
  </head>
  <body>
  	<div id="container">
	    <%- include('header.ejs',{cartTotal: cartTotal}); %>
	    <h3>Receipt</h3>
		<div class = "cart">
		</div>
		<br>
		<div id = "couponList">
		</div>
		<div>
			<h3>Total Price:
			<span id="totalprice"><%= cartTotal%></span></h3>
		</div>

		<br>
		<div class = "deliveryInfo">
			<h3> Delivery Info </h3>
			<%= user.country %> <%= user.address %>,
			<%= user.apt %> <%= user.zipCode %>
		</div>
		<div class = "paymentInfo">
			<h3> Payment Type </h3>
			<%= user.order ? user.order.paymentType : "und" %>
		</div>



		<h2>Thank you for shopping at Triangle food service</h2>

	    <%- include('footer.ejs',{}); %>
	</div>




<script type="text/javascript" src = '/receiptJS.js'></script>
<script type="text/javascript" src = '/itemDatas.js'></script>

  </body>
</html>
